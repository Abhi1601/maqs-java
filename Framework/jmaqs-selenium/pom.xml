			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.magenic.jmaqs.baseTest</groupId>
			<artifactId>jmaqs-baseTest</artifactId>
			<version>0.0.1-SNAPSHOT</version>
